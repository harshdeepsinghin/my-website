      <guid>http://localhost:1313/socials/</guid>
      <description>&lt;!DOCTYPE html&gt;&#xA;&lt;html lang=&#34;en&#34;&gt;&#xA;&lt;head&gt;&#xA;&lt;meta charset=&#34;UTF-8&#34; /&gt;&#xA;&lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1&#34; /&gt;&#xA;&lt;title&gt;Harshdeep&#39;s Socials&lt;/title&gt;&#xA;&lt;style&gt;&#xA;  /* Terminal style colors */&#xA;&#xA;  #socials-title {&#xA;    font-size: 30px;&#xA;    margin-bottom: 20px;&#xA;    text-align: center;&#xA;  }&#xA;&#xA;  img.profile-photo {&#xA;    display: block;&#xA;    margin: 1.5rem auto;&#xA;    width: 150px;&#xA;    height: 150px;&#xA;    border: 1px solid #00ff00; /* neon green border */&#xA;    object-fit: cover;&#xA;  }&#xA;&#xA;  .toggle-buttons {&#xA;    display: flex;&#xA;    justify-content: center;&#xA;    gap: 1rem;&#xA;  }&#xA;&#xA;  .toggle-buttons button {&#xA;    background-color: #121212;&#xA;    color: #00ff00;&#xA;    border: 2px solid #00ff00;&#xA;    padding: 0.5rem 0.5rem;&#xA;    font-family: &#39;Fira Code&#39;, monospace;&#xA;    font-weight: bold;&#xA;    cursor: pointer;&#xA;    transition: all 0.3s ease;&#xA;  }&#xA;&#xA;  .toggle-buttons button.active {&#xA;    background-color: #00ff00;&#xA;    color: #1e1e1e;&#xA;  }&#xA;&#xA;  .slider-wrapper {&#xA;    width: 90vw;&#xA;    max-width: 800px;&#xA;    margin: 0 auto;&#xA;    overflow: hidden;&#xA;    position: relative;&#xA;    transition: height 0.3s ease;&#xA;  }&#xA;&#xA;  .slider-pane {&#xA;    padding: 1rem;&#xA;    box-sizing: border-box;&#xA;    display: none;&#xA;    justify-content: center;&#xA;    align-items: center;&#xA;  }&#xA;&#xA;  .slider-pane.active {&#xA;    display: block;&#xA;  }&#xA;&#xA;  ul.social-list {&#xA;    list-style: none;&#xA;    padding: 0;&#xA;    margin: 0 auto;&#xA;    max-width: 500px;&#xA;    text-align: center;&#xA;  }&#xA;&#xA;  ul.social-list li {&#xA;    margin: 0.7rem 0;&#xA;  }&#xA;&#xA;  ul.social-list a {&#xA;    display: block;&#xA;    padding: 0.7rem 1rem;&#xA;    color: #00ff00;&#xA;    background-color: #121212;&#xA;    border: 1.5px solid #00ff00;&#xA;    text-decoration: none;&#xA;    font-weight: 600;&#xA;    font-size: 1rem;&#xA;    transition: background-color 0.3s ease, color 0.3s ease;&#xA;    text-align: left;&#xA;  }&#xA;&#xA;  ul.social-list a:hover {&#xA;    background-color: #00ff00;&#xA;    color: #1e1e1e;&#xA;    box-shadow: 0 0 8px #00ff00;&#xA;  }&#xA;&#xA;  ul.social-list img.favicon {&#xA;    width: 20px;&#xA;    height: 20px;&#xA;    margin-right: 10px;&#xA;    filter: invert(67%) sepia(91%) saturate(394%) hue-rotate(77deg) brightness(94%) contrast(88%);&#xA;    transition: filter 0.3s ease;&#xA;  }&#xA;&#xA;  ul.social-list a:hover .favicon {&#xA;    filter: none;&#xA;  }&#xA;&#xA;  /* Responsive */&#xA;  @media only screen and (max-width: 800px) {&#xA;    #socials-title {&#xA;      font-size: 18px;&#xA;      margin-bottom: 10px;&#xA;    }&#xA;    ul.social-list {&#xA;      max-width: 90%;&#xA;      width: 90vw;&#xA;    }&#xA;  }&#xA;&lt;/style&gt;&#xA;&lt;/head&gt;&#xA;&lt;body&gt;&#xA;  &lt;img&#xA;    src=&#34;https://harshdeepsingh.sirv.com/Images/profile-photo-3.jpeg&#34;&#xA;    alt=&#34;Harshdeep Singh&#34;&#xA;    class=&#34;profile-photo&#34;&#xA;  /&gt;&#xA;  &lt;h1 id=&#34;socials-title&#34;&gt;Connect with Harshdeep Singh&lt;/h1&gt;&#xA;&#xA;  &lt;div class=&#34;toggle-buttons&#34;&gt;&#xA;    &lt;button id=&#34;btn-community&#34;&gt;Communities&lt;/button&gt;&#xA;    &lt;button id=&#34;btn-professional&#34; class=&#34;active&#34;&gt;Professional&lt;/button&gt;&#xA;    &lt;button id=&#34;btn-personal&#34;&gt;Personal&lt;/button&gt;&#xA;  &lt;/div&gt;&#xA;&#xA;  &lt;div class=&#34;slider-wrapper&#34; id=&#34;sliderWrapper&#34;&gt;&#xA;    &lt;div class=&#34;slider-container&#34; id=&#34;slider&#34;&gt;&#xA;&#xA;      &lt;!-- Community Section --&gt;&#xA;      &lt;div class=&#34;slider-pane&#34; id=&#34;community&#34;&gt;&#xA;        &lt;ul class=&#34;social-list&#34;&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://t.me/linuxwale&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/2111/2111646.png&#34; class=&#34;favicon&#34; /&gt; Telegram - LinuxWale&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://t.me/quotesthathitushard&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/2111/2111646.png&#34; class=&#34;favicon&#34; /&gt; Telegram - Quotes That Hit Us Hard&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://t.me/hiphopindiacommunity&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/2111/2111646.png&#34; class=&#34;favicon&#34; /&gt; Telegram - Hip Hop India Community&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;&#xA;        &lt;/ul&gt;&#xA;      &lt;/div&gt;&#xA;&#xA;      &lt;!-- Professional Section --&gt;&#xA;      &lt;div class=&#34;slider-pane active&#34; id=&#34;professional&#34;&gt;&#xA;        &lt;ul class=&#34;social-list&#34;&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;mailto:contact@harshdeepsingh.in&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/732/732200.png&#34; class=&#34;favicon&#34; /&gt; Email&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://www.linkedin.com/in/harshdeepsinghin/&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/174/174857.png&#34; class=&#34;favicon&#34; /&gt; LinkedIn&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://github.com/harshdeepsinghin&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/733/733553.png&#34; class=&#34;favicon&#34; /&gt; GitHub&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://www.instagram.com/harshdeepsinghin&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/174/174855.png&#34; class=&#34;favicon&#34; /&gt; Instagram&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;        &lt;/ul&gt;&#xA;      &lt;/div&gt;&#xA;&#xA;&#xA;      &lt;!-- Personal Section --&gt;&#xA;      &lt;div class=&#34;slider-pane&#34; id=&#34;personal&#34;&gt;&#xA;        &lt;ul class=&#34;social-list&#34;&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://t.me/harshluckyme&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/2111/2111646.png&#34; class=&#34;favicon&#34; /&gt; Telegram&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://www.instagram.com/harshluckyme&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/174/174855.png&#34; class=&#34;favicon&#34; /&gt; Instagram&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &lt;li&gt;&lt;a href=&#34;https://open.spotify.com/user/zgearbiszvaowgdl6yvyx362k&#34; target=&#34;_blank&#34;&gt;&#xA;            &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/174/174872.png&#34; class=&#34;favicon&#34; /&gt; Spotify&#xA;          &lt;/a&gt;&lt;/li&gt;&#xA;          &#xA;        &lt;/ul&gt;&#xA;      &lt;/div&gt;&#xA;    &lt;/div&gt;&#xA;  &lt;/div&gt;&#xA;&#xA;  &lt;script&gt;&#xA;  const btnCommunity = document.getElementById(&#39;btn-community&#39;);&#xA;  const btnProfessional = document.getElementById(&#39;btn-professional&#39;);&#xA;  const btnPersonal = document.getElementById(&#39;btn-personal&#39;);&#xA;  const wrapper = document.getElementById(&#39;sliderWrapper&#39;);&#xA;  const panes = {&#xA;    community: document.getElementById(&#39;community&#39;),&#xA;    professional: document.getElementById(&#39;professional&#39;),&#xA;    personal: document.getElementById(&#39;personal&#39;)&#xA;  };&#xA;&#xA;  function showPane(type) {&#xA;    Object.values(panes).forEach(p =&gt; p.classList.remove(&#39;active&#39;));&#xA;    panes[type].classList.add(&#39;active&#39;);&#xA;    wrapper.style.height = panes[type].scrollHeight + &#39;px&#39;;&#xA;    btnCommunity.classList.toggle(&#39;active&#39;, type === &#39;community&#39;);&#xA;    btnProfessional.classList.toggle(&#39;active&#39;, type === &#39;professional&#39;);&#xA;    btnPersonal.classList.toggle(&#39;active&#39;, type === &#39;personal&#39;);&#xA;  }&#xA;&#xA;  window.addEventListener(&#39;DOMContentLoaded&#39;, () =&gt; {&#xA;    showPane(&#39;professional&#39;);&#xA;  });&#xA;&#xA;  btnCommunity.addEventListener(&#39;click&#39;, () =&gt; showPane(&#39;community&#39;));&#xA;  btnProfessional.addEventListener(&#39;click&#39;, () =&gt; showPane(&#39;professional&#39;));&#xA;  btnPersonal.addEventListener(&#39;click&#39;, () =&gt; showPane(&#39;personal&#39;));&#xA;  &lt;/script&gt;&#xA;&lt;/body&gt;&#xA;&lt;/html&gt;</description>
